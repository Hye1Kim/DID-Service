  "source": "pragma solidity 0.5.6;\n\ncontract DIDStorage{\n\n    // mapping(string => Document[]) documents; //key: 신원인증고유갑, value: 분산id\n    // mapping(string => DIDState) didState;\n    // mapping(string => Service[]) services;\n    // mapping(string => uint) public nonce;\n\n    // enum DIDState {None, Active, Deactivated}\n\n    //     struct PublicKey{\n    //     string id;\n    //     string keyType;\n    //     string pubKeyData;\n    //     bool disable;\n    // }\n\n    // struct Document{\n    //     string[] contexts;  //\"https://www.w3.org/ns/did/v1\" 왜 배열이야 ?\n    //     string id;\n    //     PublicKey[] publicKeys;\n    //     string[] credentials;\n    // }\n\n    // struct Service{\n    //     string id;\n    //     PublicKey[] publicKeys;\n    //     string serviceType; //issuer, verifier\n    //     string endPoint;\n    //     bool disable;\n    // }\n\n\n\n    mapping(string => Document) documents;\n    mapping(string => DIDState) didState;\n    mapping(string => uint) public nonce;\n    \n    enum DIDState {None, Active, Deactivated}\n\n    struct PublicKey{\n        string id;\n        string keyType;\n        string pubKeyData;\n        bool disable;\n    }\n    \n    struct Service{\n        string id;\n        string serviceType;\n        string serviceEndPoint;\n        PublicKey[] publickeys;\n        bool disable;\n    }\n    \n    struct Document{\n        string id;\n        string[] contexts;  //\"https://www.w3.org/ns/did/v1\" 왜 배열이야 ?\n        PublicKey[] publicKeys;\n        string[] credentials;\n    }\n\n\n}\n",
